group_count: 2
atr1_unknown: 0
entries:
  QL_RitoMini_Flint_Desc:
    contents:
      - text: "到利特村度蜜月的久戈\n想为妻子准备烤苹果。\n\n要做烤苹果自然需要用火。\n\n拿"
      - control:
          kind: set_colour
          colour: red
      - text: 打火石
      - control:
          kind: reset_colour
      - text: 给他点火吧。
  QL_RitoMini_Flint_Finish:
    contents:
      - text: "你把打火石交给了\n来利特村度蜜月的久戈，\n获得卢比作为谢礼！\n\n久戈好像打算多做点烤苹果。\n\n获得"
      - control:
          kind: set_colour
          colour: red
      - text: 打火石
      - control:
          kind: reset_colour
      - text: "后，\n一起拿去给他吧！"
  QL_RitoMini_Flint_Name:
    contents:
      - text: 新郎挽回名誉
