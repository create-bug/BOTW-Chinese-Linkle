group_count: 3
atr1_unknown: 0
entries:
  QL_HatenoMini_LoveInsects_Answer:
    contents:
      - text: "茨琪米的梦想是\n在100只"
      - control:
          kind: set_colour
          colour: red
      - text: 精力蚱蜢
      - control:
          kind: reset_colour
      - text: "包围下生活。\n\n将这件事告诉"
      - control:
          kind: set_colour
          colour: red
      - text: 万作
      - control:
          kind: reset_colour
      - text: 吧。
  QL_HatenoMini_LoveInsects_Desc:
    contents:
      - text: "万作的朋友的朋友\n好像非常非常地想引起\n东风亭店主茨琪米的注意。\n\n"
      - control:
          kind: set_colour
          colour: red
      - text: 和茨琪米对话
      - control:
          kind: reset_colour
      - text: "，\n不知能不能打探出\n她喜欢的东西呢？"
  QL_HatenoMini_LoveInsects_Finish:
    contents:
      - text: "将10只精力蚱蜢交给万作，\n获得谢礼。\n\n结果想引起茨琪米注意的\n好像就是万作本人。\n\n剩下的精力蚱蜢由他自己去找吧。"
  QL_HatenoMini_LoveInsects_Name:
    contents:
      - text: 送给爱人的礼物
  QL_HatenoMini_LoveInsects_Report:
    contents:
      - text: "茨琪米的梦想是\n在100只精力蚱蜢包围下生活。\n知道这件事之后，\n万作拜托你帮忙收集蚱蜢。\n\n总之先收集"
      - control:
          kind: set_colour
          colour: red
      - text: 10只精力蚱蜢
      - control:
          kind: reset_colour
      - text: "\n拿去万作那里吧。"
