group_count: 3
atr1_unknown: 0
entries:
  QL_GotoZoraVillage_Desc:
    contents:
      - text: "卓拉族的王子希多拜托你\n"
      - control:
          kind: set_colour
          colour: red
      - text: 去卓拉领地一趟
      - control:
          kind: reset_colour
      - text: "。\n\n但是因为暴雨的缘故，\n沿途的悬崖变得非常湿滑。"
  QL_GotoZoraVillage_Finish:
    contents:
      - text: "你来到了卓拉领地的王座跟前。\n\n按希多的话来看，\n卓拉领地已经陷入了危机，\n需要一个强大的海利亚人出手相助。"
  QL_GotoZoraVillage_Name:
    contents:
      - text: 千辛万苦走到卓拉领地
  QL_GotoZoraVillage_Step1:
    contents:
      - text: "你来到了卓拉领地。\n\n多莱凡王好像在王座那里。\n"
      - control:
          kind: set_colour
          colour: red
      - text: 去上层的王座
      - control:
          kind: reset_colour
      - text: 那里吧。
