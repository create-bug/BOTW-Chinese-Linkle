group_count: 2
atr1_unknown: 0
entries:
  QL_RitoMini_Cook_Desc:
    contents:
      - text: "到利特村度蜜月的茱诺，\n因村里没有她想要的东西而非常生气。\n\n但若能吃到最爱的"
      - control:
          kind: set_colour
          colour: red
      - text: 烤苹果
      - control:
          kind: reset_colour
      - text: "，\n她应该会消气吧。\n\n为了拯救她的婚姻，\n拿"
      - control:
          kind: set_colour
          colour: red
      - text: 烤苹果
      - control:
          kind: reset_colour
      - text: 给她吧！
  QL_RitoMini_Cook_Finish:
    contents:
      - text: "你将烤苹果交给了\n来利特村度蜜月的茱诺！\n\n茱诺振振有词，烤苹果是装进另一个胃的。\n\n多做点"
      - control:
          kind: set_colour
          colour: red
      - text: 烤苹果
      - control:
          kind: reset_colour
      - text: "，\n再一起拿给她吧！"
  QL_RitoMini_Cook_Name:
    contents:
      - text: 新娘舍华求实
