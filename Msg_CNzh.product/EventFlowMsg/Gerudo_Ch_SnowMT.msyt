          cancel_index: 1
          unknown: 6
  Talk03:
    attributes: Npc_OasisHylia_002
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - control:
          kind: text_size
          percent: 80
      - text: "很多人都调查过第八位英雄的事，\n但至今那仍是谜团重重的都市传说。\n"
      - control:
          kind: set_colour
          colour: grey
      - text: "我要趁着她调查谜团的期间培养感情……\n然后……"
      - control:
          kind: reset_colour
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: set_colour
          colour: grey
      - control:
          kind: pause
          length: longer
      - control:
          kind: reset_colour
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: 呵呵！
  Talk05:
    attributes: Npc_OasisHylia_002
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: 太好了！
      - control:
          kind: pause
          length: short
      - text: "\n你平安无事！"
      - control:
          kind: pause
          length: longer
      - text: "\n你一直没回来，\n让我好担心……"
      - control:
          kind: pause
          length: longer
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: 其实
      - control:
          kind: set_colour
          colour: blue
      - text: 第八位英雄
      - control:
          kind: reset_colour
      - text: "不过是则都市传说，\n根本不存在……\n我……"
      - control:
          kind: pause
          length: short
      - text: "\n只是想和你拉近关系，\n所以才提出这么过分的要求，\n想借此留住你……\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "我一直好担心好担心，\n就怕害你遇上什么危险……\n不过我不会再让你涉险了！\n今后就由我来保……"
      - control:
          kind: single_choice
          label: 2
  Talk06:
    attributes: Npc_OasisHylia_002
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "你是怕我担心，\n所以才这样安慰我吗？\n你可真善良。"
      - control:
          kind: pause
          length: longer
      - text: "\n\n不过你不必再顾虑我了，\n今后就由我来保……"
      - control:
          kind: single_choice
          label: 8
