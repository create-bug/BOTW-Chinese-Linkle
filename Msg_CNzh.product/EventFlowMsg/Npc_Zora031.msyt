  talk08:
    attributes: Npc_Zora031
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: 难道说……
      - control:
          kind: pause
          length: short
      - text: "林可大人……\n\n你不记得自己曾在100年前，与米法大人一同"
      - control:
          kind: pause
          length: short
      - text: "\n对抗"
      - control:
          kind: set_colour
          colour: red
      - text: 灾厄盖侬
      - control:
          kind: reset_colour
      - text: "的事了卓啦？\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: 林可大人……
      - control:
          kind: pause
          length: short
      - text: "你真的都不记得了卓啦……\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: 好！
      - control:
          kind: pause
          length: longer
      - text: "\n既然如此，有什么问题尽管问我卓啦。"
      - control:
          kind: choice
          choice_labels:
            - 5
