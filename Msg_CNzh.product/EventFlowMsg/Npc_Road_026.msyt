          unknown:
            - 1
            - 0
      - text: 听说会游得比较快哦……
      - control:
          kind: pause
          length: longer
      - control:
          kind: pause
          length: longer
      - text: 要是裸体的话。
  MSG_Rane_04_1:
    attributes: Npc_Road_026
    contents:
      - control:
