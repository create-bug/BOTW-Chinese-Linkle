            - 7
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Talk06:
    attributes: Npc_RitoHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: 请好好休息！
  Talk07:
    attributes: Npc_RitoHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: 那么，请好好休息！！
  Talk08:
    attributes: Npc_RitoHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: 早上好，有睡饱吗？
      - control:
          kind: pause
          length: longer
      - text: "\n路上请小心。"
  Talk09:
