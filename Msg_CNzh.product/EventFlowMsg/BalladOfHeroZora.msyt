atr1_unknown: 4
entries:
  "0000":
    attributes: ""
    contents:
      - text: 想询问米法的事
  "0001":
    attributes: ""
    contents:
      - text: 再见
  "0002":
    attributes: ""
    contents:
      - text: 告诉我试练的事
  "0003":
    attributes: ""
    contents:
      - text: 日记？
  Npc_Zora001_SD_000:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 20
      - text: 哦哦，
      - control:
          kind: pause
          length: short
      - text: 这不是英杰大人吗！
      - control:
          kind: pause
          length: short
      - text: "\n没想到能在这里见到您！"
      - control:
          kind: pause
          length: short
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: 我是为担任
      - control:
          kind: set_colour
          colour: blue
      - text: 慕兹力阁下
      - control:
          kind: reset_colour
      - text: "的护卫而来的……\n \n"
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: 听说因为此处曾有
      - control:
          kind: set_colour
          colour: blue
      - text: 米法大人
      - control:
          kind: reset_colour
      - text: 接受过的试练……
      - control:
          kind: pause
          length: long
      - text: "\n\n详细情形就请问"
      - control:
          kind: set_colour
          colour: blue
      - text: 慕兹力阁下
      - control:
          kind: reset_colour
      - text: 吧。
  Npc_Zora001_SD_100:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: 一路前来非常辛苦……
      - control:
          kind: pause
          length: long
      - text: "\n沿途不知遭怪物袭击了几次……"
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: 但慕兹力阁下始终干劲满满，
      - control:
          kind: pause
          length: short
      - text: "\n说“这一定能帮上希多王子”。"
  Npc_Zora001_SD_200:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "虽然慕兹力阁下\n要我先做好回程的准备……\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: 但我一想到那趟路程的艰辛……
      - control:
          kind: pause
          length: long
  Npc_Zora001_SD_300:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 我们曾离开过
      - control:
          kind: set_colour
          colour: blue
      - text: 卓拉领地
      - control:
          kind: reset_colour
      - text: "，\n但领地外还是危险丛生。\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 4
      - text: 我也要努力加强领地的警备！
  Npc_Zora001_SD_301:
    attributes: Npc_Zora001
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: 慕兹力阁下可是很开心，
      - control:
          kind: pause
          length: short
      - text: "\n说“能帮上希多王子”呢。"
  Npc_Zora003_SD_000:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 4
      - text: 林克，如果你有什么难处，
      - control:
          kind: pause
          frames: 20
      - text: "\n随时都可以来领地。\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "无论何时，\n我们一族上下都欢迎你的到来卓啦！\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 那么事不宜迟……
      - control:
          kind: pause
          length: long
      - text: "\n有什么我能效劳之处吗？"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_Zora003_SD_010:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: 呼，好吧卓哟……
      - control:
          kind: pause
          length: long
      - text: 让你听听
      - control:
          kind: set_colour
          colour: blue
      - text: 米法
      - control:
          kind: reset_colour
      - text: "的事，\n也算是祭吊她的在天之灵卓哟。\n"
      - control:
          kind: set_colour
          colour: blue
      - text: 米法
      - control:
          kind: reset_colour
      - text: 性情温柔……
      - control:
          kind: pause
          length: long
      - text: "无论是对领地的人\n还是对旅行者……她都一视同仁亲切相待卓哟。\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "年幼的你造访这里时，\n米法也待你如胞弟，对你疼爱有加。\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "然而，海利亚人成长得比我们快，\n那孩子开始得抬头仰望你……"
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: 如同
      - control:
          kind: set_colour
          colour: blue
      - text: 米法的日记
      - control:
          kind: reset_colour
      - text: 中所记载……
      - control:
          kind: pause
          length: long
      - text: "\n她日渐对你心生恋慕之情卓哟。\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "卓拉公主爱上了海利亚人的剑士……\n族中也曾有过这样的佳话卓啦。\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: 所以我也曾想过，若你与
      - control:
          kind: set_colour
          colour: blue
      - text: 米法
      - control:
          kind: reset_colour
      - text: "能结为夫妻，\n也不失为美事一桩……"
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: 真是遗憾啊卓啦……
      - control:
          kind: pause
          length: long
      - control:
          kind: single_choice
          label: 3
  Npc_Zora003_SD_015:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "嗯，是最近发现的卓哟……我可高兴了。\n"
      - control:
          kind: set_colour
          colour: blue
      - text: 米法的日记
      - control:
          kind: reset_colour
      - text: 就放在
      - control:
          kind: set_colour
          colour: blue
      - text: 那边的桌子上
      - control:
          kind: reset_colour
      - text: "卓哟。\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "既然是你的话，看看也无妨……\n"
      - control:
          kind: set_colour
          colour: blue
      - text: 米法
      - control:
          kind: reset_colour
      - text: 肯定也会同意的卓哟。
  Npc_Zora003_SD_020:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "如果有什么难处，随时都可以来。\n我们已经视你如已出卓哟。"
