          unknown: 6
  FirstTalk01:
    attributes: Npc_King_Vagrant006
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - control:
          kind: sound2
          unknown:
            - 8
            - 205
      - text: 嚯、嚯、嚯。
      - control:
          kind: pause
          length: long
      - text: "\n毕竟姜还是老的辣啊。"
