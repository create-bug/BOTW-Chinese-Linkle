    contents:
      - text: 嗯！
  Clear1_00:
    attributes: Npc_King_Vagrant006
    contents:
      - control:
          kind: sound2
          unknown:
            - 10
            - 205
      - text: "回答得倒是蛮自信的，\n但你只通过了1座神庙。"
      - control:
          kind: choice
          choice_labels:
            - 1002
            - 1003
