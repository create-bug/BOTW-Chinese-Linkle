          cancel_index: 1
          unknown: 6
  FirstTalk00:
    attributes: Npc_King_Vagrant006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "竟然输给我这个老人家，\n小伙子你还要多努力啊。"
