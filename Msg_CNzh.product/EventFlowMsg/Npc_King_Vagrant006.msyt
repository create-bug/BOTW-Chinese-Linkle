          cancel_index: 1
          unknown: 6
  Clear2_00:
    attributes: Npc_King_Vagrant006
    contents:
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: 回答得不错。
      - control:
          kind: pause
          length: long
      - text: "\n你好像通过了2座神庙。"
      - control:
          kind: choice
          choice_labels:
            - 1002
            - 1003
