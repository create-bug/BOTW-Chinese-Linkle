  talk17:
    attributes: Npc_UMiiVillage008
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 对面那户人家呀，
      - control:
          kind: pause
          length: longer
      - text: "\n夫妇俩好像"
      - control:
          kind: set_colour
          colour: red
      - text: 每天晚上
      - control:
