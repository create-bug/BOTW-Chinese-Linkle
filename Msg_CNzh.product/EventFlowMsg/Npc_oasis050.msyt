      - control:
          kind: pause
          length: short
      - text: 你说对吧？
  talk44:
    attributes: Npc_oasis050
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 哎呀，这样啊……
  talk45:
    attributes: Npc_oasis050
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
