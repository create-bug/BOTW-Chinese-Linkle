      - text: 滑翔帆
      - control:
          kind: reset_colour
      - text: 的话，你就得好好努力。
  Kotsu00:
    attributes: Npc_King_Parasail004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - control:
          kind: animation
          name: CustomLaugh
      - text: 嚯、嚯、嚯。
      - control:
          kind: pause
          length: long
      - text: "\n活得时间长了，就什么都略知一二了。\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: 去吧，赶快去试试看吧。
  Kotsu01:
    attributes: Npc_King_Parasail004
    contents:
      - text: 打开
      - control:
          kind: set_colour
          colour: blue
      - text: 希卡之石
      - control:
          kind: reset_colour
      - text: 里的
      - control:
          kind: set_colour
          colour: blue
      - text: 地图
      - control:
          kind: reset_colour
      - text: "，\n选择标有蓝色标记的塔。\n"
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: 这样的话，你就能瞬间移动到那里了……
      - control:
          kind: pause
          length: long
      - text: "\n你去试试吧。"
  NakedPlayer:
    attributes: ""
