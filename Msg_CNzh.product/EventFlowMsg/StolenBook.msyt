  Npc_Kakariko002_R_003:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: 你叫我打起精神？
      - control:
          kind: pause
          length: short
      - text: "\n哪来的力气打起精神！"
      - control:
          kind: pause
          length: longer
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - control:
          kind: set_colour
          colour: blue
      - text: 宝珠
      - control:
          kind: reset_colour
      - text: ……
      - control:
