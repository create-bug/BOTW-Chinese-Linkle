  talk000:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: 早啊！欢迎来到
      - control:
          kind: set_colour
          colour: blue
      - text: 塔邦挞大桥驿站
      - control:
          kind: reset_colour
      - text: 。
      - control:
          kind: pause
          length: short
      - text: "\n怎么样？我家的白山羊很可爱吧？"
  talk001:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 你好！欢迎来到
      - control:
          kind: set_colour
          colour: blue
      - text: 塔邦挞大桥驿站
      - control:
          kind: reset_colour
      - text: 。
      - control:
          kind: pause
          length: short
      - text: "\n我正在检查重要的货物。"
  talk002:
    attributes: Npc_TabantaBridgeHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: 来了来了，欢迎来到
      - control:
          kind: set_colour
