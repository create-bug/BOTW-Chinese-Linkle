      - control:
          kind: set_colour
          colour: blue
      - text: 防火药
      - control:
          kind: reset_colour
      - text: 。
  talk16:
    attributes: Npc_DeathEastHatago_003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: 咦……？
      - control:
          kind: pause
          length: longer
      - text: "\n你身上的东西"
      - control:
          kind: pause
          length: short
      - text: "太多了吧？\n不要的东西就卖掉或是丢掉吧。"
  talk17:
    attributes: Npc_DeathEastHatago_003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 好的，
      - control:
          kind: pause
          length: short
      - text: 那这个给你。
  talk18:
    attributes: Npc_DeathEastHatago_003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: 如果你要前往鼓隆城，
      - control:
          kind: pause
          length: short
      - text: "\n我就把"
      - control:
          kind: set_colour
          colour: blue
      - text: 防火药
      - control:
          kind: reset_colour
      - text: 卖给你吧？
  talk19:
    attributes: Npc_DeathEastHatago_003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: 嗯……
      - control:
          kind: pause
          length: short
      - text: "这样啊，但是……\n"
  talk20:
    attributes: Npc_DeathEastHatago_003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 你好像
      - control:
          kind: pause
          length: short
      - text: 已经有很多
      - control:
          kind: set_colour
          colour: blue
      - text: 防火药
      - control:
          kind: reset_colour
      - text: 了。
      - control:
          kind: pause
          length: short
      - text: "\n我想应该可以撑到鼓隆城吧。"
