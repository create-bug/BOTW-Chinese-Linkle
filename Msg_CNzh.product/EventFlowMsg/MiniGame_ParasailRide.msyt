      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 14
          name: MiniGame_ParasailRide_PassedTargetNumTemp
      - text: 个圆环
      - control:
          kind: reset_colour
      - text: ！
      - control:
          kind: pause
          length: long
      - text: "\n你真的是海利亚人吗？"
  Result_04_01:
    attributes: Npc_FarthestIsland002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 这是你让我大开眼界的谢礼！
      - control:
          kind: pause
          length: long
      - text: "\n希望你能收下。"
  Suggest_Bay:
    attributes: Npc_FarthestIsland002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 现在的话……
      - control:
          kind: pause
          length: short
