  AskMoney_00:
    attributes: Npc_FarthestIsland002
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: 我很高兴你感兴趣。
  AskMoney_01:
    attributes: Npc_FarthestIsland002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 训练需要花费
      - control:
          kind: set_colour
          colour: red
      - text: 20卢比
      - control:
          kind: reset_colour
      - text: 做准备，
      - control:
          kind: pause
          length: short
      - text: "\n这样可以吗？"
