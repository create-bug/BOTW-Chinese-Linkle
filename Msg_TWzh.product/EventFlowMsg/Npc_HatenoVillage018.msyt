          length: short
      - text: 那道
      - control:
          kind: set_colour
          colour: blue
      - text: 藍色火焰
      - control:
          kind: reset_colour
      - text: ，
      - control:
          kind: pause
          length: short
      - text: "\n是普爾亞婆婆的……"
  talk12:
    attributes: Npc_HatenoVillage018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: 你快把那道
      - control:
          kind: set_colour
          colour: blue
      - text: 藍色火焰
      - control:
          kind: reset_colour
      - text: ……
      - control:
          kind: pause
          length: short
      - text: "\n帶去給普爾亞婆婆吧。"
  talk13:
    attributes: Npc_HatenoVillage018
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: 哎呀呀……
      - control:
          kind: pause
          length: short
