              field_3: 8
      - text: "せきぞう石像 "
      - control:
          kind: raw
          zero:
            zero:
              field_1: 8
              field_2: 2
              field_3: 4
      - text: やみ闇の
      - control:
          kind: raw
          zero:
            zero:
              field_1: 10
              field_2: 2
              field_3: 6
      - text: ひかり光が
      - control:
          kind: raw
          zero:
            zero:
              field_1: 8
